{
  "name": "rag-talk-rag-api",
  "version": "1.0.0",
  "description": "RAG API server for RAG talk demo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "load-data": "node load-sample-data.js",
    "test-embeddings": "node test-local-embeddings.js",
    "test-rag-modes": "node test-rag-modes.js",
